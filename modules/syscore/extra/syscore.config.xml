<!-- Configuration file for the syscore module -->
<bashdata fni="_initglobals_syscore">
    <var name="SHELLAPI_LOCALE">en</var>
    <var name="SHELLAPI_PAYLOAD">lzma</var>
    <var name="SHELLAPI_HOME">${SHELLAPI_HOME:-.}</var>
    <var name="SHELLAPI_FNOIMP">n</var>
    <var name="SHELLAPI_MODULES_DIR">${SHELLAPI_HOME}/modules</var>
    <var name="SHELLAPI_MLOG" check="warn">${SHELLAPI_HOME}/logging/shellapi.log</var>
    <var name="SHELLAPI_VERBOSITY">${SHELLAPI_VERBOSITY:=max}</var>
    <var name="SHELLAPI_LOGFMSG">${SHELLAPI_LOGFMSG=y}</var>
    <var name="SHCORE_MLOG">${SHELLAPI_HOME}/logging/shellapi.log</var>
    <var name="SHELLAPI_HASH_MODES">${SHELLAPI_HASH_MODES:=sha1sum}</var>
    <var name="SHELLAPI_HASH">${SHELLAPI_HASH_MODES}</var>
    <var name="SHELLAPI_RSPACE">${SHELLAPI_TARGET}/shellrun</var>
    <var name="I9KG_POOLSPACE">${SHELLAPI_TARGET}/poolset</var>
    <var name="POOL_RELAY_STORE">${SHELLAPI_TARGET}/relays</var>
    <var name="POOL_RELAY_CACHE">${SHELLAPI_TARGET}/relays/cache</var>
    <var name="_I9KG_RLAY">0</var>
    <var name="_I9KG_POOL">1</var>
    <var name="_I9KG_RHID">2</var>
    <var name="_I9KG_PHID">3</var>
    <var name="_I9KG_EXPR">4</var>
    <var name="_HPAGE">0</var>
    <var name="_CHECKSUM">1</var>
    <var name="_ENTRY">2</var>
    <var name="_UPDATE">3</var>
    <var name="_PREGET">4</var>
    <var name="_POSTGET">5</var>
    <array name="SHELLAPI_MODULES"/>
    <array name="SHELLAPI_LFUNC"/>
    <array name="SHELLAPI_GMSG"/>
    <array name="I9KG_DEFS" check="ignore">
        <index name="PROGRESS_LOCKS">${SHELLAPI_RSPACE}/progress/locks</index>
        <index name="PROGRESS_REPORTS">${SHELLAPI_RSPACE}/progress/reports</index>
        <index name="PROCESS_LOCKS">${SHELLAPI_RSPACE}/process/locks</index>
        <index name="PROCESS_REPORTS">${SHELLAPI_RSPACE}/process/reports</index>
        <index name="BTCHAIN">${SHELLAPI_RSPACE}/btchain</index>
        <index name="POOLSETS">${SHELLAPI_TARGET}/poolset</index>
    </array>
    <array name="I9KG_PRIME" check="fatal">
        <index name="LOCATION">${I9KG_POOLSPACE}/prime</index>
        <index name="PRISTINE">${I9KG_POOLSPACE}/prime/pristine</index>
        <index name="PAYLOAD">${I9KG_POOLSPACE}/prime/payload</index>
        <index name="PATCHES">${I9KG_POOLSPACE}/prime/patches</index>
        <index name="SNAPSHOTS">${I9KG_POOLSPACE}/prime/snapshots</index>
        <index name="CLONES">${I9KG_POOLSPACE}/prime/clones</index>
        <index name="EZCONFIG">${I9KG_POOLSPACE}/prime/ezconfig</index>
        <index name="METABASE">${I9KG_POOLSPACE}/prime/metabase</index>
        <index name="METACACHE">${I9KG_POOLSPACE}/prime/metacache</index>
        <index name="I9KG_DEPOT">${I9KG_POOLSPACE}/prime/i9kg</index>
        <index name="I9KG_SEEDS">${I9KG_POOLSPACE}/prime/i9kg/seeds</index>
        <index name="I9KG_SEEDS_XML">${I9KG_POOLSPACE}/prime/i9kg/seeds/xml</index>
        <index name="I9KG_SEEDS_JSON">${I9KG_POOLSPACE}/prime/i9kg/seeds/json</index>
        <index name="I9KG_SEEDS_YAML">${I9KG_POOLSPACE}/prime/i9kg/seeds/yaml</index>
        <index name="I9KG_REPORTS">${I9KG_POOLSPACE}/prime/i9kg/reports</index>
        <index name="I9KG_REPORTS_XML">${I9KG_POOLSPACE}/prime/i9kg/reports/xml</index>
        <index name="I9KG_REPORTS_JSON">${I9KG_POOLSPACE}/prime/i9kg/reports/json</index>
        <index name="I9KG_REPORTS_YAML">${I9KG_POOLSPACE}/prime/i9kg/reports/yaml</index>
        <index name="FCACHE">${I9KG_POOLSPACE}/prime/fcache</index>
        <index name="RHID"></index>
        <index name="RPLI"></index>
    </array>
    <array name="I9KG_ALIASES">
        <index>LOCATION</index>
        <index>PRISTINE</index>
        <index>PAYLOAD</index>
        <index>PATCHES</index>
        <index>SNAPSHOTS</index>
        <index>CLONES</index>
        <index>EZCONFIG</index>
        <index>METABASE</index>
        <index>METACACHE</index>
        <index>I9KG_DEPOT</index>
        <index>I9KG_SEEDS</index>
        <index>I9KG_SEEDS_XML</index>
        <index>I9KG_SEEDS_JSON</index>
        <index>I9KG_SEEDS_YAML</index>
        <index>I9KG_REPORTS</index>
        <index>I9KG_REPORTS_XML</index>
        <index>I9KG_REPORTS_JSON</index>
        <index>I9KG_REPORTS_YAML</index>
        <index>FCACHE</index>
        <index>RHID</index>
        <index>RPLI</index>
    </array>
    <array name="I9KG_OAL">
        <index>RLAY</index>
        <index>POOL</index>
        <index>RHID</index>
        <index>PHID</index>
        <index>EXPR</index>
    </array>
    <array name="I9KG_UTILSPACE" check="reuse">
        <index name="LOCATION">${SHELLAPI_RSPACE}/utilspace</index>
    </array>
</bashdata>
